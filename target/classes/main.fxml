<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="body" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.primaryController">
   <children>
      <Label layoutX="242.0" layoutY="81.0" prefHeight="44.0" prefWidth="201.0" stylesheets="@style.css" text="Welcome" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Bodoni MT Bold Italic" size="36.0" />
         </font>
      </Label>
      <Button fx:id="clockBtn" layoutX="243.0" layoutY="167.0" mnemonicParsing="false" onAction="#clickOnClock" prefHeight="20.0" prefWidth="100.0" stylesheets="@style.css" text="Clock" textFill="AZURE" />
      <Button fx:id="calcBtn" layoutX="243.0" layoutY="217.0" mnemonicParsing="false" onAction="#clickOnCalc" prefHeight="30.0" prefWidth="100.0" stylesheets="@style.css" text="Calculator" textFill="AZURE" />
      <Button fx:id="ImageSliderBtn" layoutX="243.0" layoutY="264.0" mnemonicParsing="false" onAction="#clickOnImageSlider" prefHeight="30.0" prefWidth="100.0" text="Image Slider" />
   </children>
</AnchorPane>
